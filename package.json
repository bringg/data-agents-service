{
	"name": "data-agents-service",
	"version": "1.0.0",
	"description": "",
	"main": "dist/server.js",
	"private": true,
	"engines": {
		"node": ">=20.11.0",
		"npm": ">=10.2.4"
	},
	"scripts": {
		"start": "node -r @bringg/service/bootstrap dist/server.js",
		"start-dev": "generate-config-local && node_modules/.bin/nodemon",
		"prestart": "generate-config-prod",
		"build": "tsc",
		"test": "nyc --reporter=cobertura ts-mocha",
		"prettier": "prettier --write \"{app,test}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"prettier-check": "prettier -c \"{app,test}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"eslint-check": "eslint \"{app,test}/**/*\"",
		"eslint-fix": "eslint --fix \"{app,test}/**/*\"",
		"ts-check": "tsc --noEmit"
	},
	"eslintConfig": {
		"extends": "@bringg/eslint-config/be"
	},
	"author": "",
	"prettier": "@bringg/prettier-config",
	"lint-staged": {
		"*.md": [
			"npm run markdownlint-fix",
			"git add"
		],
		"{app,test}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"npm run eslint-fix",
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"@bringg/eslint-config": "^1.7.3",
		"@bringg/prettier-config": "^1.1.2",
		"@commitlint/cli": "^19.4.1",
		"@commitlint/config-conventional": "^17.0.0",
		"@types/chai": "^4.3.16",
		"@types/chai-as-promised": "^7.1.8",
		"@types/express": "^4.16.1",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.16",
		"@types/sinon-chai": "^3.2.12",
		"chai": "^4.5.0",
		"chai-as-promised": "^7.1.2",
		"express": "^4.21.2",
		"husky": "^8.0.3",
		"lint-staged": "^13.0.4",
		"mocha": "^10.4.0",
		"mocha-junit-reporter": "^2.2.1",
		"mocha-multi-reporters": "^1.5.1",
		"nodemon": "^3.0.1",
		"nyc": "^17.1.0",
		"prettier": "^2.8.3",
		"sinon-chai": "^3.7.0",
		"supertest": "^7.0.0",
		"ts-mocha": "^10.0.0",
		"ts-node": "^10.9.2",
		"ts-prune": "^0.10.3",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.9.5"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/bringg/data-agents-service.git"
	},
	"dependencies": {
		"@bringg/service": "^3.35.1",
		"@bringg/service-utils": "^4.55.0",
		"@bringg/test-utils": "^1.12.0",
		"@bringg/types": "^4.185.0",
		"@langchain/core": "^0.3.40",
		"@langchain/google-vertexai": "^0.2.0",
		"@langchain/langgraph": "^0.2.44",
		"@langchain/openai": "^0.4.3",
		"@types/cors": "^2.8.17",
		"axios": "^1.8.4",
		"cors": "^2.8.5",
		"uuid": "^11.0.5"
	}
}
